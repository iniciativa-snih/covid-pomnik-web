    "cross-env": "^7.0.3",
    "express": "^4.17.1",
    "lodash": "^4.17.21",
    "moment": "^2.29.1",
    "next": "^10.0.8",
    "next-plausible": "^1.5.1",
    "next-seo": "^4.22.0",
    "numeraljs": "^1.5.6",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-hamburger-menu": "^1.2.1",
    "react-scroll-parallax": "^2.3.5",
    "react-visibility-sensor": "^5.1.1"
  },
  "devDependencies": {
